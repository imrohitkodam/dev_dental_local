			<field
			name="statuslist"
			type="fsjchecklist"
			label=""
			description=""
			use_set=""
			size=""
			default=""
									
										fsjchecklist_sql = "SELECT title as display, id FROM #__fss_ticket_status ORDER BY ordering"	
						>	

												</field>
	
		
			<field
			name="frame_fields"
			type="fsjframe"
