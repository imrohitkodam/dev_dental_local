        <description></description>
      </item>-->
    </section>
    <section nonmenusection="1" name="Report Builder">
      <item id="report" icon="report">
        <title>COM_FSJ_FSSADD_DATA_REPORT</title>
        <description/>
      </item>
    </section>
  </admin>
  <tables>
    <table name="canned" id="1" title="1" access="1" language="1" state="1" alias="1" asset_id="1" ordering="1">
      <field name="description" type="text"/>
      <field name="notes" type="text"/>
      <field name="usestatus" type="int(11)"/>
      <field name="subject" type="varchar(250)"/>
      <field name="category" type="varchar(250)"/>
      <field name="statuslist" type="varchar(250)"/>
