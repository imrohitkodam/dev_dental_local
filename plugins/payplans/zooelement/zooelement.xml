<?xml version="1.0" encoding="utf-8"?>
<extension version="3.1" type="plugin" group="payplans" method="upgrade">
	<name>PayPlans - Zoo Element</name>
	<version>4.0.0</version>
	<creationDate>6th August 2018</creationDate>
	<author>StackIdeas</author>
	<authorEmail>support@stackideas.com</authorEmail>
	<authorUrl>https://stackideas.com</authorUrl>
	<copyright>Copyright 2009 - 2018 Stack Ideas Sdn Bhd. All Rights Reserved.</copyright>
	<license>http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL</license>
	<description>Through this plugin you can control the display of element according to plan subscribed by user</description>
	<files>
		<folder>zooelement</folder>
		<filename plugin="zooelement">zooelement.php</filename>
		<filename>zooelement.xml</filename>
		<filename>install.sql</filename>
	</files>
	<install>
		<sql>
			<file driver="mysql" charset="utf8">install.sql</file>
		</sql>
	</install>
	<updateservers>
		<server type="extension" priority="1" name="StackIdeas Modules and Plugins">https://stackideas.com/joomla4compat.xml</server>
	</updateservers>
</extension>